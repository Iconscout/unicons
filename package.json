  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Iconscout/unicons/issues"
  },
  "homepage": "https://iconscout.com/unicons",
  "devDependencies": {},
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "fontello-cli": "^0.4.0",
    "fs-plus": "^3.1.1",
    "glob": "^7.1.3",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-svgstore": "^2.0.0",
    "lodash": "^4.17.11",
    "parse-svg-path": "^0.1.2",
    "scale-svg-path": "0.0.1",

{
  "name": "unicons",
  "version": "0.0.4",
  "description": "Ready to use font icons for your next project",
  "scripts": {
    "build": "npm run download && npm run generate-fontello-config && npm run generate",
    "download": "node build/download.js",
    "generate": "fontello-cli install && node build/folder-move.js && grunt generate-sprite",
    "generate-sprite": "grunt generate-sprite",
    "generate-fontello-config": "node build/generate-fontello-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Iconscout/unicons.git"
  },
  "keywords": [
    "icon",
    "font",
    "font",
    "icon",
    "icons",
    "webfont"
  ],
